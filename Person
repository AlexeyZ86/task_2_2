package lesson_2.q_2;
public class Person {
    private String name;
    private String surname;
    private String[] phones;
    private Address address;
    
    public Person() {}
}

    public Address getAddress() {
        return address;
    }

    public String[] getPhones() {
        return phones;
    }

    public String getSurname() {
        return surname;
    }

    public String getName() {
        return name;
    }

   
